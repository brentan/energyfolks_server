
#efadminbar_spacer {
  padding:0px;margin:0px;height:28px;border-width:0px;
}

/* The following styles the black login bar that will show at the top of the page */
#efadminbar *{
  height:auto;width:auto;margin:0;padding:0;text-transform:none;letter-spacing:normal;line-height:1;font:normal 13px/28px sans-serif;color:#ccc;text-shadow:#444 0 -1px 0;
}
#eftoplogin *{
  padding:3px;text-align:left;color:#444444;text-shadow:none;line-height:1.15;
}
#eftoplogin h3{
  text-align:center;font-size:16px;
}
#eftoplogin h1{
  text-align:center;font-size:18px;
}
#eftoplogin h6{
  text-align:center;font-size:13px;
}
#eftoplogin a{
  color:#777777;
}
#eftoplogin a:hover{
  color:#222222;
}
#efadminbar ul li:before,#efadminbar ul li:after{
  content:normal;
}
#efadminbar a,#efadminbar a:hover,#efadminbar a img,#efadminbar a img:hover{
  outline:none;border:none;text-decoration:none;background:none;
}
#efadminbar{
  direction:ltr;color:#ccc;font:normal 13px/28px sans-serif;height:28px;width:100%;min-width:600px;position:fixed;top:0px;left:0px;z-index:99999;background-image:url(<%= SITE_HOST %>/images/black_fade_70.png);
}
#efadminbar ul,#efadminbar ul li{
  background:none;clear:none;list-style:none;margin:0;padding:0;position:relative;z-index:99999;
}
#efadminbar .quicklinks{
  border-left:1px solid transparent;
}
#efadminbar .quicklinks ul{
  text-align:left;
}
#efadminbar li{
  float:left;
}
#efadminbar .quicklinks>ul>li{
  border-right:1px solid #555;
}
#efadminbar .quicklinks>ul>li>a,#efadminbar .quicklinks>ul>li>.ab-empty-item{
  border-right:1px solid #333;
}
#efadminbar .quicklinks .ab-top-secondary>li{
  border-left:1px solid #333;border-right:0;float:right;
}
#efadminbar .quicklinks .ab-top-secondary>li>a,#efadminbar .quicklinks .ab-top-secondary>li>.ab-empty-item{
  border-left:1px solid #555;border-right:0;
}
#efadminbar .quicklinks a {
  height:28px;display:block;padding:0 12px;margin:0;
}
#efadminbar .menupop .ab-sub-wrapper{
  margin:0 0 0 -1px;padding:0;-moz-box-shadow:0 4px 4px rgba(0,0,0,0.2);-webkit-box-shadow:0 4px 4px rgba(0,0,0,0.2);box-shadow:0 4px 4px rgba(0,0,0,0.2);background:#fff;display:none;position:absolute;float:none;border-width:0 1px 1px 1px;border-style:solid;border-color:#dfdfdf;
}
#efadminbar.ie7 .menupop .ab-sub-wrapper,#efadminbar.ie7 .shortlink-input{top:28px;left:0;}
#efadminbar .ab-sub-wrapper>.ab-submenu:first-child{border-top:none;}
#efadminbar .ab-submenu{padding:6px 0;border-top:1px solid #dfdfdf;}
#efadminbar .selected .shortlink-input{display:block;}
#efadminbar .quicklinks .menupop ul li{float:none;}
#efadminbar .quicklinks .menupop ul li a strong{font-weight:bold;}
